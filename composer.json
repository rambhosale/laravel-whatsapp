{
    "name": "rambhosale/laravel-whatsapp",
    "description": "Use Whatsapp's Cloud API",
    "keywords": [
        "rambhosale",
        "laravel-whatsapp"
    ],
    "homepage": "https://github.com/rambhosale/laravel-whatsapp",
    "license": "MIT",
    "authors": [
        {
            "name": "rambhosale",
            "email": "zenosamav2.0@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^7.0 | ^8.0 | ^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rambhosale\\Whatsapp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rambhosale\\Whatsapp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
